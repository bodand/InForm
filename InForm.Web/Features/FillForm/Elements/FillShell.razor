<div class="border-solid border-2 border-slate-400 p-4">
    <h3 class="text-2xl md:text-3xl w-fit break-words">@Element.Title</h3>
    <h4 class="text-base md:text-lg w-fit break-words">@Element.Subtitle</h4>

    <div class="mt-6">
        @ChildContent
    </div>
</div>


@code {
    [Parameter, EditorRequired]
    public ElementModel Element { get; set; } = default!;

    [Parameter, EditorRequired]
    public RenderFragment ChildContent { get; set; } = default!;
}
